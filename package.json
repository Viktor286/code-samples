{
  "name": "codesamples",
  "version": "1.0.0",
  "description": "Different code samples",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Viktor286/code-samples"
  },
  "engines": {
    "node": ">= 12.16.1"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "jest --coverage",
    "lint": "concurrently \"npm run lint:src\" \"npm run lint:spec\"",
    "format": "prettier \"src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint:src": "eslint src/**/*.ts --fix --ignore-pattern *.spec.ts",
    "lint:spec": "eslint **/**.spec.ts -c .eslintrc.spec.js --fix",
    "test:watch": "jest --coverage --watchAll",
    "tpsc": "tsc -w --listFiles --traceResolution",
    "precommit": "npm run lint && npm test && npm run format"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ]
}
